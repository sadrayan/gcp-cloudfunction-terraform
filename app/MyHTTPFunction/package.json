{
  "author": "Sadrayan",
  "name": "my-http-function",
  "version": "0.1.0",
  "main": "dist/index.js",
  "scripts": {
    "lint": "tslint --project tslint.json",
    "tsc": "tsc --project tsconfig.json",
    "cp-package": "copyfiles * -e \"**/*.test.ts\"  dist",
    "build": "run-s lint tsc cp-package",
    "clean": "rm -rf dist",
    "echo": "$npm_package_name",
    "zip:ci": "cd dist && zip -r -j $npm_package_name.zip *",
    "build:ci": "npm run clean && run-s lint tsc cp-package && npm run zip:ci"
  },
  "keywords": [
    "gcp",
    "cloud functions",
    "terraform",
    "typescript"
  ],
  "description": "This is a TypeScript based Cloud Functions template.",
  "dependencies": {
    "express": "^4.18.2",
    "googleapis": "^118.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@google-cloud/functions-framework": "^3.2.0",
    "@google-cloud/logging": "^10.5.0",
    "@google-cloud/logging-winston": "^5.3.0",
    "@google-cloud/pubsub": "^3.7.1",
    "@google-cloud/rcloadenv": "^3.0.1",
    "@google-cloud/storage": "^6.11.0",
    "@google/events": "^5.4.0",
    "@types/express": "^4.17.11",
    "@types/node": "^20.3.1",
    "@types/request": "^2.48.5",
    "copyfiles": "^2.4.1",
    "npm-run-all": "^4.1.5",
    "tslint": "^6.1.3",
    "typescript": "^5.1.3",
    "winston": "^3.3.3"
  }
}
